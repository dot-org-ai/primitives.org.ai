{
  "name": "ai-database",
  "version": "2.1.4",
  "description": "AI-powered database interface primitives with mdxld conventions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./tests": {
      "import": "./dist/tests.js",
      "types": "./dist/tests.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/primitives.org.ai.git",
    "directory": "packages/ai-database"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@graphdl/core": "workspace:*",
    "@org.ai/types": "workspace:*",
    "ai-functions": "workspace:*",
    "digital-objects": "workspace:*",
    "hyparquet": "^1.24.1",
    "hyparquet-writer": "^0.12.0",
    "mdxld": "^1.0.1",
    "rpc.do": "^0.2.3"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20250124.0",
    "vitest": "3.0.4"
  },
  "peerDependencies": {
    "vitest": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "keywords": [
    "ai",
    "database",
    "linked-data",
    "mdxld",
    "primitives"
  ],
  "license": "MIT"
}
