{
  "name": "ai-workflows",
  "version": "2.1.3",
  "description": "Event-driven workflows with state machine support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@org.ai/types": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "ai",
    "workflows",
    "events",
    "state-machine",
    "primitives"
  ],
  "license": "MIT"
}
