{
  "name": "ai-evaluate",
  "version": "2.1.8",
  "description": "Secure code execution in sandboxed environments",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts"
    },
    "./repl": {
      "import": "./dist/repl.js",
      "types": "./dist/repl.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "capnweb": "^0.2.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.0",
    "vitest": "^2.1.0"
  },
  "optionalDependencies": {
    "esbuild": "^0.24.0",
    "miniflare": "^3.20241106.0"
  },
  "keywords": [
    "ai",
    "sandbox",
    "evaluate",
    "cloudflare",
    "workers",
    "miniflare",
    "primitives"
  ],
  "license": "MIT"
}
