{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "eval-workers-do",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-01",
  "account_id": "b6641681fe423910342b9ffa1364c76d",

  // Custom domain
  "routes": [
    { "pattern": "eval.workers.do", "custom_domain": true }
