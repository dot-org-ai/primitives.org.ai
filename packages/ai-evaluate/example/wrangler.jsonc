  // Keep workers.dev enabled for testing
  "workers_dev": true,

  // Worker loader for dynamic sandbox execution
  "worker_loaders": [
    { "binding": "loader" }
  ],

  // Bundle configuration to resolve workspace package
  "alias": {
    "ai-evaluate": "../dist/index.js"
  }
}
