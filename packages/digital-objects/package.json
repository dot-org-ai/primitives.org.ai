{
  "name": "digital-objects",
  "version": "1.0.0",
  "description": "Unified storage primitive for AI primitives - nouns, verbs, things, actions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ns": {
      "types": "./dist/ns-exports.d.ts",
      "import": "./dist/ns-exports.js"
    },
    "./worker": {
      "types": "./dist/worker-exports.d.ts",
      "import": "./dist/worker-exports.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "digital-objects",
    "durable-objects",
    "cloudflare",
    "sqlite",
    "graph",
    "entity",
    "nouns",
    "verbs",
    "things",
    "actions",
    "linguistics",
    "ai-primitives"
  ],
  "author": ".org.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/org-ai/primitives.git",
    "directory": "packages/digital-objects"
  },
  "homepage": "https://primitives.org.ai/digital-objects",
  "bugs": {
    "url": "https://github.com/org-ai/primitives/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20240925.0",
    "vitest": "^2.0.0",
    "wrangler": "^3.79.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
