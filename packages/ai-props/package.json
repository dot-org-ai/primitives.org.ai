{
  "name": "ai-props",
  "version": "2.1.4",
  "description": "AI-powered props primitives for intelligent component properties",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    },
    "./mdx": {
      "import": "./dist/mdx.js",
      "types": "./dist/mdx.d.ts"
    },
    "./streaming": {
      "import": "./dist/streaming.js",
      "types": "./dist/streaming.d.ts"
    },
    "./rpc": {
      "import": "./dist/rpc.js",
      "types": "./dist/rpc.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:worker": "vitest --config vitest.workers.config.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "ai-providers": "workspace:*",
    "language-models": "workspace:*",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.7.2",
    "vitest": "3.0.4"
  },
  "keywords": [
    "ai",
    "props",
    "react",
    "primitives"
  ],
  "license": "MIT"
}
