{
  "name": "ai-primitives",
  "version": "1.0.0",
  "description": "All AI primitives for building intelligent applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:e2e": "vitest run --config e2e/node/vitest.config.ts",
    "test:e2e:node": "vitest run --config e2e/node/vitest.config.ts",
    "test:e2e:browser": "vitest run --config e2e/browser/vitest.config.ts",
    "test:e2e:worker": "vitest run --config e2e/worker/vitest.config.ts",
    "test:e2e:all": "pnpm test:e2e:node && pnpm test:e2e:browser && pnpm test:e2e:worker"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "ai-providers": "workspace:*",
    "language-models": "workspace:*",
    "ai-database": "workspace:*",
    "ai-workflows": "workspace:*",
    "ai-experiments": "workspace:*",
    "autonomous-agents": "workspace:*",
    "digital-workers": "workspace:*",
    "digital-tasks": "workspace:*",
    "digital-tools": "workspace:*",
    "digital-products": "workspace:*",
    "human-in-the-loop": "workspace:*",
    "services-as-software": "workspace:*",
    "business-as-code": "workspace:*",
    "ai-props": "workspace:*",
    "ai-evaluate": "workspace:*",
    "org.ai": "workspace:*"
  }
}
