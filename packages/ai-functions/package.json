{
  "name": "ai-functions",
  "version": "0.0.1",
  "description": "Core AI primitives for building intelligent applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rpc": {
      "import": "./dist/rpc/index.js",
      "types": "./dist/rpc/index.d.ts"
    },
    "./embeddings": {
      "import": "./dist/embeddings.js",
      "types": "./dist/embeddings.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/cloudflare": {
      "import": "./dist/providers/cloudflare.js",
      "types": "./dist/providers/cloudflare.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured'",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^4.0.0",
    "ai-providers": "workspace:*",
    "capnweb": "^0.2.0"
  },
  "optionalDependencies": {
    "oauth.do": "^0.0.1"
  },
  "keywords": ["ai", "functions", "rpc", "capnweb", "embeddings", "cloudflare", "oauth", "primitives"],
  "license": "MIT"
}
